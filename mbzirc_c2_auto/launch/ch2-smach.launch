<node name="state_machine" pkg="mbzirc_c2_state" type="mbzirc_simulation_state_machine_2.py" />

<node name="board" pkg="mbzirc_c2_auto" type="panel.sh" />
<node name="image_view" pkg="image_view" type="image_view">
